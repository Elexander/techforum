<h1>New Private Message</h1>


<%= form_for :privatemessage do |f| %>
  <p>
    <%= f.label :recipent %><br>
    <%= f.text_field :recipent %>
  </p>
 <p>
    <%= f.label :subject %><br>
    <%= f.text_field :subject %>
  </p>
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

